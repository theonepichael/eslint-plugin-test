{
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.run": "onType",
  "eslint.debug": true,
  "eslint.workingDirectories": [
    { "directory": "eslint-plugin-test", "changeProcessCWD": true }
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.associations": {
    "*.js": "javascript",
    "*.ts": "typescript"
  },
  "eslint.options": {
    "extensions": [".js", ".ts"]
  }
}
